{
    "name": "service/delivery",
    "description": "Counting cost of the delivery cargos",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "henry1479"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "require": {
        "laravie/parser": "^2.0",
        "geocoder-php/google-maps-provider": "^4.7",
        "guzzlehttp/guzzle": "^7.8",
        "php-http/guzzle7-adapter": "^1.0",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
